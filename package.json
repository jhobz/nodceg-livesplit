    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "GPL-3.0",
  "dependencies": {
    "animejs": "^3.2.1",
    "fitty": "^2.3.6",
    "livesplit-client": "satanch/node-livesplit-client#df280d726f5fc9cf5690c91f44a24a466e5f1a25"
  }
