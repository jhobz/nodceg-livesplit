{
  "name": "livesplit",
  "version": "0.0.0",
  "description": "Connects to a local instance of LiveSplit to display information or control the timer",
  "homepage": "https://github.com/jhobz/nodecg-livesplit",
  "author": {
    "name": "Jeff Hobson",
    "email": "jeffrey296@gmail.com",
    "url": "https://github.com/jhobz"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack"
  },
  "nodecg": {
    "assetCategories": [
      {
        "name": "logos",
        "title": "Logos",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ],
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "livesplit",
        "title": "LiveSplit",
        "width": 4,
        "file": "livesplit.html"
      }
    ],
    "graphics": [
      {
        "file": "widget.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "GPL-3.0",
  "dependencies": {
    "animejs": "^3.2.1",
    "fitty": "^2.3.6",
    "livesplit-client": "../../../node-livesplit-client"
  },
  "devDependencies": {
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
