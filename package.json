{
  "name": "livesplit",
  "version": "0.0.0",
  "description": "Connects to a local instance of LiveSplit to display information or control the timer",
  "homepage": "https://github.com/jhobz/nodecg-livesplit",
  "author": {
    "name": "Jeff Hobson",
    "email": "jeffrey296@gmail.com",
    "url": "https://github.com/jhobz"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
